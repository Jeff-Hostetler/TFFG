<div class="siteBody">
  <h3>{{firstName}} {{lastName}}</h3>
  <hr>
  <div class="siteBody">
    <h4>Shifts scheduled</h4>
    <hr>
    {{#each shift in shifts}}
     {{shift.startTime}}  until {{ shift.endTime}}
     <br>
    {{/each}}

    <br></br>

    <h4>Contact info</h4>
    <hr>
    <p>
      Email: {{email}}
    </p>
    <p>
      Phone Number: {{phoneNumber}}
    </p>
  </div>
</div>
