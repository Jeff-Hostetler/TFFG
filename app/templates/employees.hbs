<div class="siteBody siteTop">
  <div class="header">
    <div class="new-link">

        {{#link-to 'employees.new'}}
          New Employee
        {{/link-to}}

    </div>
    <h3>Employees</h3>
  </div>
  <hr>

  {{#each employee in model}}
    <div class="tableRow">
      {{#link-to 'employee' employee}}
      {{employee.firstName}} {{employee.lastName}} - {{employee.position}}
      {{/link-to}}
    </div>

  {{/each}}
</div>

{{outlet}}
